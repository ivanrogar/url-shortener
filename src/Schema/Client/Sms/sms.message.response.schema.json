{
  "$id": "sms.message.response.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SMS response schema",
  "type": "object",
  "properties": {
    "bulkId": {
      "type": "string"
    },
    "messages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "to": {
            "type": "string"
          },
          "status": {
            "type": "object",
            "properties": {
              "groupId": {
                "type": "integer"
              },
              "groupName": {
                "type": "string"
              },
              "id": {
                "type": "integer"
              },
              "name": {
                "type": "string"
              },
              "description": {
                "type": "string"
              }
            }
          },
          "messageId": {
            "type": "string"
          }
        }
      }
    }
  }
}
